{
  "name": "chainflow",
  "version": "0.1.0",
  "description": "build dynamic and flexible API call workflows",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "commitlint": "commitlint --edit",
    "lint": "eslint ./src/ --ext .ts",
    "lint:fix": "eslint ./src/ --ext .ts --fix",
    "markdownlint": "markdownlint **/*.md --ignore node_modules",
    "prepare": "husky install",
    "test": "glob -c \"node --import tsx --test --no-warnings \" \"./src/**/*.test.ts\"",
    "test:file": "glob -c \"node --import tsx --test --no-warnings \"",
    "test:coverage": "glob -c \"c8 node --test --import tsx --no-warnings\" \"./src/**/*.test.ts\"",
    "test:debug": "glob -c \"node --import tsx --no-warnings --test\" \"./src/**/*.test.ts\"",
    "demo": "tsc && node ./dist/demo/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/deepmerge": "^1.3.0",
    "debug": "^4.3.4",
    "undici": "^6.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.8.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.19.1",
    "c8": "^9.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "markdownlint-cli": "^0.37.0",
    "prettier": "^3.0.3",
    "tsx": "4.7.0",
    "typescript": "^5.2.2"
  }
}
