{
  "name": "chainflow",
  "version": "0.1.0",
  "description": "build dynamic and flexible API call workflows",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "commitlint": "commitlint --edit",
    "docs": "typedoc --options ./docs/typedoc/typedoc.json",
    "docs:post": "ts-node ./docs/typedoc/postprocess.ts",
    "lint": "eslint ./src/ --ext .ts",
    "lint:fix": "eslint ./src/ --ext .ts --fix",
    "markdownlint": "markdownlint **/*.md --ignore node_modules --ignore docs/api",
    "prepare": "husky install",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "demo": "tsc && node ./dist/demo/index.js"
  },
  "keywords": [],
  "author": "edwinlzs",
  "license": "ISC",
  "dependencies": {
    "@fastify/deepmerge": "^1.3.0",
    "debug": "^4.3.4",
    "undici": "^6.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "markdownlint-cli": "^0.37.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.7",
    "typedoc-plugin-frontmatter": "^0.0.2",
    "typedoc-plugin-markdown": "4.0.0-next.52",
    "typescript": "^5.3.3"
  }
}
